hs, setMonths] = useState(\"\");\n\n  const [isCard, setIsCard] = useState(false);\n  const dispatch = useDispatch();\n\n  const cart = useSelector((state) =>\n    state.cart.map((product) => ({\n      ...product,\n      subtotal: product.price * product.amount,\n    }))\n  );\n\n  const total = useSelector((state) =>\n    state.cart.reduce((totalSum, product) => {\n      return totalSum + product.price * product.amount;\n    }, 0)\n  );\n  const [totalDesconto, setTotalDesconto] = useState(total);\n\n  function increment(product) {\n    dispatch(\n      CartActions.updateAmount({\n        id: product.id,\n        amount: product.amount + 1,\n      })\n    );\n  }\n\n  function decrement(book) {\n    dispatch(\n      CartActions.updateAmount({\n        id: book.id,\n        amount: book.amount - 1,\n      })\n    );\n  }\n\n  function setPaymentMethod(value) {\n    setPayment(value);\n    value === \"boleto\" ? setIsCard(false) : setIsCard(true);\n  }\n\n  function validarCupom() {\n    const isValid = { valid: true, desconto: 0.3 }; // faz uma chamada na api pra validar o cupom\n    if (isValid.valid) {\n      setTotalDesconto(total - total * isValid.desconto);\n      message(\"Cupom aplicado :)\");\n    } else {\n      message(\"Cupom invalido :(\");\n    }\n  }\n\n  return (\n    <main className=\"container\">\n      <div className=\"cart-container\">\n        <table className=\"product-table\">\n          <thead>\n            <tr>\n              <th />\n              <th>Produto</th>\n              <th>Quantidade</th>\n              <th>Subtotal</th>\n            </tr>\n          </thead>\n          <tbody>\n            {cart.map((product) => (\n              <tr key={product.id}>\n                <td>\n                  <button\n                    type=\"button\"\n                    onClick={() =>\n                      dispatch(CartActions.removeFromCart(product.id))\n                    }\n                  >\n                    <FiXCircle size={20} color=\"#33BFCB\" />\n                  </button>\n                </td>\n\n                <td>\n                  <strong>\n                    {product.name} - R$ {product.price}\n                  </strong>\n                </td>\n                <td>\n                  <div>\n                    <button type=\"button\" onClick={() => decrement(product)}>\n                      <FiMinusCircle size={20} color=\"#33BFCB\" />\n                    </button>\n                    <input type=\"number\" readOnly value={product.amount} />\n                    <button type=\"button\" onClick={() => increment(product)}>\n                      <FiPlusCircle size={20} color=\"#33BFCB\" />\n                    </button>\n                  </div>\n                </td>\n                <td>\n                  <strong>R$ {product.subtotal.toFixed(3).slice(0, -1)}</strong>\n                </td>\n              </tr>\n            ))}\n          </tbody>\n        </table>\n\n        <div className=\"payment-container\">\n          <div>\n            <p>Forma de pagamento:</p>\n            <select\n              name=\"payment\"\n              onChange={(e) => setPaymentMethod(e.target.value)}\n            >\n              <option value=\"boleto\">Boleto Bancário</option>\n              <option value=\"cartão\">Cartão de Crédito</option>\n            </select>\n          </div>\n\n          <div className=\"total\">\n            <span>Total: R$ {total.toFixed(3).slice(0, -1)}</span>\n            <span>\n              Pagamento:{\" \"}\n              {payment === \"boleto\"\n                ? \"Boleto bancário\"\n                : \"Cartão de crédito, \" +\n                  months +\n                  \" x \" +\n                  (totalDesconto / months).toFixed(3).slice(0, -1)}\n            </span>\n          </div>\n        </div>\n        {isCard ? (\n          <div className=\"card-container\">\n            <input\n              placeholder=\"Nome do titular do cartão\"\n              value={name}\n              onChange={(e) => setName(e.target.value)}\n              required\n            />\n            <input\n              placeholder=\"Número do cartão\"\n              value={cardNumber}\n              onChange={(e) => setCardNumber(e.target.value)}\n              required\n            />\n            <input\n              placeholder=\"Código de segurança\"\n              value={sCode}\n              onChange={(e) => setSCode(e.target.value)}\n              required\n            />\n            <input\n              placeholder=\"Validade dd/aa\"\n              value={vData}\n              onChange={(e) => setVData(e.target.value)}\n              required\n            />\n            <input\n              placeholder=\"Parcelas\"\n              value={months}\n              type=\"number\"\n              min=\"1\"\n              max=\"12\"\n              onChange={(e) => setMonths(e.target.value)}\n              required\n            />\n          </div>\n        ) : (\n          <></>\n        )}\n\n        <footer>\n          <button type=\"button\">Finalizar Compra</button>\n        </footer>\n      </div>\n    </main>\n  );\n}\n\nexport default Cart;\n","/home/caio/Documentos/Projetos_Particulares/Lojinha/web/src/store/index.js",[],"/home/caio/Documentos/Projetos_Particulares/Lojinha/web/src/store/modules/rootReducer.js",[],"/home/caio/Documentos/Projetos_Particulares/Lojinha/web/src/store/modules/cart/actions.js",[],"/home/caio/Documentos/Projetos_Particulares/Lojinha/web/src/store/modules/cart/reducer.js",[],"/home/caio/Documentos/Projetos_Particulares/Lojinha/web/src/services/auth.js",[],"/home/caio/Documentos/Projetos_Particulares/Lojinha/web/src/store/modules/auth/actions.js",[],"/home/caio/Documentos/Projetos_Particulares/Lojinha/web/src/store/modules/auth/reducer.js",[],{"ruleId":"76","replacedBy":"77"},{"ruleId":"78","replacedBy":"79"},{"ruleId":"76","replacedBy":"80"},{"ruleId":"78","replacedBy":"81"},{"ruleId":"82","severit